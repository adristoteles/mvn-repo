<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <name>SADI service tester</name>
  <description>Sonatype helps open source projects to set up Maven repositories on https://oss.sonatype.org/</description>
  <groupId>org.sadiframework</groupId>
  <artifactId>sadi-tester</artifactId>
  <version>0.3.0</version>
  <goalPrefix>sadi-tester</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>test-service</goal>
      <description>A Maven plugin to test a SADI service.</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <implementation>org.sadiframework.service.tester.TestService</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>expectedPath</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The URL or local path of the expected output RDF document corresponding
to the input document specified above.
This parameter is optional.</description>
        </parameter>
        <parameter>
          <name>inputPath</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The URL or local path of an input RDF document.
This parameter is optional.</description>
        </parameter>
        <parameter>
          <name>oldExpectedPath</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>serviceURL</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The URL of the service to be tested.
This parameter is required.</description>
        </parameter>
      </parameters>
      <configuration>
        <expectedPath implementation="java.lang.String" default-value="">${expected}</expectedPath>
        <inputPath implementation="java.lang.String" default-value="">${input}</inputPath>
        <oldExpectedPath implementation="java.lang.String">${output} default-value=</oldExpectedPath>
        <serviceURL implementation="java.lang.String">${serviceURL}</serviceURL>
      </configuration>
    </mojo>
    <mojo>
      <goal>help</goal>
      <description>A goal that describes the goals and parameters of the plugin.</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <implementation>org.sadiframework.service.tester.Help</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters/>
    </mojo>
  </mojos>
